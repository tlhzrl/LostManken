    <script src="~/Adminpage/startup/js/jquery.slicknav.min.js"></script>
    <script src="~/Adminpage/startup/js/jquery.magnific-popup.min.js"></script>
    <script src="~/Adminpage/startup/js/plugins.js"></script>
    <script src="~/Adminpage/startup/js/gijgo.min.js"></script>

    <script src="~/Adminpage/startup/js/contact.js"></script>
